pipeline {
    agent any
    tools {
        maven 'maven'  // Use the exact nyou set in Global Tool Configuration
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('deliver') {
            steps {
                sh'./scripts/deliver.sh'
            }
        }
    }
}
